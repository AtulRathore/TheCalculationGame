<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atulRtech.atul.thecalcualtiongame">


    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.atulRtech.atul.thecalcualtiongame.Splash"
            android:label="@string/app_name"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.atulRtech.atul.thecalcualtiongame.SelectGame"
            android:screenOrientation="portrait"/>

        <activity android:name="com.atulRtech.atul.thecalcualtiongame.MainActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.atulRtech.atul.thecalcualtiongame.Multiplication"
            android:screenOrientation="portrait"/>
    </application>

</manifest>